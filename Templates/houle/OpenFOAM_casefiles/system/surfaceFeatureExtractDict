FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeatureExtractDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


%geometry%.stl
{
	extractionMethod    extractFromSurface;

	extractFromSurfaceCoeffs
	{
		includedAngle 150;
	}

	subsetFeatures
	{
		nonManifoldEdges	yes;
		openEdges		yes;
	}
	

	writeObs yes;
}




