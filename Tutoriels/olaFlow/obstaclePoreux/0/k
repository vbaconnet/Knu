FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.00000001;

boundaryField
{
    inlet
    {
        type            inletOutlet;
        inletValue      uniform 0.00000001;
        value           uniform 0.00000001;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.00000001;
        value           uniform 0.00000001;
    }
    "wall."
    {
        type            kqRWallFunction;
        value           uniform 0.00000001;
    }
    caisson
    {
        type            kqRWallFunction;
        value           uniform 0.00000001;
    }
    defaultFaces
    {
        type            empty;
    }
    atmosphere
    {
        type            inletOutlet;
        inletValue      uniform 0.00000001;
        value           uniform 0.00000001;
    }
}


