FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    // Selectionner toutes les cells mais pas le overset
    {
        name    c0;
        type    cellSet;
        action  new;
        source  regionsToCell;
        insidePoints ((-0.049 -0.04 0.005));
    }

    // Copier dans c1
    {
        name    c1;
        type    cellSet;
        action  new;
        source  cellToCell;
        set     c0;
    }

    // Inverser la sÃ©lection --> c1 <-> overset
    {
        name    c1;
        type    cellSet;
        action  invert;
    }

    // transformer cellSet overset en cellZone
    {
        name    oversetZone;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        sourceInfo
        {
            set c1;
        }
    }
);

// ************************************************************************* //
